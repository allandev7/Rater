<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXListView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="540.0" prefWidth="722.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.EntrevistadoresController">
   <children>
      <JFXListView fx:id="jfxlvListView" editable="true" fixedCellSize="80.0" layoutY="51.0" prefHeight="408.0" prefWidth="722.0" verticalGap="10.0" />
      <Label fx:id="lblNumEnt" layoutX="14.0" layoutY="14.0" text="Label">
         <font>
            <Font name="Tahoma" size="17.0" />
         </font>
      </Label>
      <TextField fx:id="txtPesquisarEntrevistadores" layoutX="393.0" layoutY="12.0" prefHeight="25.0" prefWidth="320.0" promptText="Pesquisar entrevistador..." />
      <HBox layoutX="24.0" layoutY="474.0" prefHeight="42.0" prefWidth="689.0">
         <children>
            <JFXButton fx:id="btnDeletarEntrevistador" onAction="#deletarEntrevistador" prefHeight="41.0" prefWidth="161.0" styleClass="btnLogin" stylesheets="@MenuCSS.css" text="Deletar">
               <HBox.margin>
                  <Insets right="100.0" />
               </HBox.margin>
            </JFXButton>
            <JFXButton fx:id="btnVisualizarPerfil" onAction="#visualizarPerfilEntrevistador" prefHeight="41.0" prefWidth="161.0" styleClass="btnLogin" stylesheets="@MenuCSS.css" text="Visualizar perfil">
               <HBox.margin>
                  <Insets right="100.0" />
               </HBox.margin>
            </JFXButton>
            <JFXButton fx:id="btnAdicionarEntrevistador" onAction="#novoEntrevistador" prefHeight="41.0" prefWidth="161.0" styleClass="btnLogin" stylesheets="@MenuCSS.css" text="Novo entrevistador" />
         </children>
      </HBox>
   </children>
</AnchorPane>
